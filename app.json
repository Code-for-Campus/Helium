{
  "expo": {
    "name": "Helium",
    "slug": "helium",
    "scheme": "helium",
    "owner": "codeforcampus",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "tech.owenwood.helium",
      "supportsTablet": false
    },
    "android": {
      "package": "tech.owenwood.helium",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/foreground.png",
        "backgroundImage": "./src/assets/images/background.png"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://helium.owenwood.tech/"
        }
      ],
      "expo-secure-store"
    ],
    "extra": {
      "router": {
        "origin": "https://helium.owenwood.tech/"
      },
      "eas": {
        "projectId": "d09f17b0-8890-4607-95ba-2501266264ca"
      }
    }
  }
}
